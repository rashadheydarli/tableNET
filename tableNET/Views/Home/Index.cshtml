

<table class="table">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Surname</th>
            <th scope="col">Age</th>
            <th scope="col">Country</th>
            <th scope="col">Phone numberr</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td scope="row">@ViewBag.Name</td>
            <td>@ViewData["Surname"]</td>
            <td>@ViewData["Age"]</td>
            <td>@ViewBag.Country</td>
            <td>@ViewBag.PhoneNumber</td>
        </tr>
    </tbody>
</table>
